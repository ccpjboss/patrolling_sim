<?xml version="1.0" encoding="UTF-8" ?>
<launch>
  <arg name="map" default="1r5" />
  <arg name="stage_pkg" default="stage_ros2"/>     <!-- stage_pkg:=stage for ROS Groovy -->

  <node name="stage" pkg="$(var stage_pkg)" exec="stage_ros2" output="screen">
	<param name="world" value="$(find-pkg-prefix patrolling_sim_ros2)/../../src/patrolling_sim_ros2/maps/$(var map)/$(var map).world"/>
  </node>
  
</launch>
